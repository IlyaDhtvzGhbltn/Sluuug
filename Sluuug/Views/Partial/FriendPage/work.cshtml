@model Slug.Model.Users.WorkPlacesModel
@using Slug.Extensions

<div class="item-info">
    <h3>@Model.CompanyTitle</h3>
    <span>@Model.Position</span>
    <span>@Model.Country @Model.City</span>
    <span>@Model.Start - @Model.End</span>
    <span class="user-comment">«@Model.Comment»</span>
</div>