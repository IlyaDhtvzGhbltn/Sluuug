@model Slug.Model.Users.EducationModel
@using Slug.Extensions

<div class="education_div">
    <p>@Model.Title <button id="@Model.EntryId" onclick="dropEntry(this)">@Html.Translate("Button_Delete")</button></p>
    <p>@Model.Country @Model.Sity</p>

    @if (Model.UntilNow == false)
    {
        <p>@Model.Start - @Model.End</p>
    }
    else
    {
        <p>@Model.Start - @Html.Translate("Text_For_Now_Time")</p>
    }
    <p><span>@Model.Comment</span></p>
</div>

