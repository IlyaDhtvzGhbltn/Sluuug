
@{
    Layout = "~/Views/Shared/Logo.cshtml";
}
<div>My messangers</div>

<div id="conversations">
    @if (Model.Cnv != null)
    {
        foreach (var item in Model.Cnv)
        {
            <div class="conver_div" id="@item.Id" style="background-color:cadetblue; cursor:grabbing">
                <img height="20" width="20" src="@item.AvatarPath" />
                <p>@item.InterlocutorName  @item.InterlocutorSurName</p>
                <p>@item.LastMessage</p>
            </div>
        }
    }
</div>
<script src="~/Scripts/jquery-1.6.4.js"></script>
<script src="~/Scripts/jquery-1.6.4.min.js"></script>
<script src="~/Scripts/goMsg.js"></script>

