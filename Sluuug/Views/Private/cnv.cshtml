
@{
    Layout = "~/Views/Shared/private_view.cshtml";
}
@model  Slug.Model.ConversationsModel
@using Slug.Extensions

<h2>@Html.Translate("Text_My_Message_Title")</h2>

<div id="conversations">
    @if (Model.Cnv != null)
    {
        foreach (var item in Model.Cnv)
        {
            Html.RenderPartial("~/Views/Partial/Dialogs/dialog_preview.cshtml", item, new ViewDataDictionary<Slug.Model.SimpleChat.CutConversation>(item));
        }
    }
</div>
<script src="~/Scripts/RelocationToUser.js"></script>
<script src="~/Scripts/newMsgInConv.js"></script>
